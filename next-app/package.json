{
  "name": "registry-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:next": "dotenvx run -f .env.development.local -- next dev --turbopack",
    "dev:next:no-turbo": "dotenvx run -f .env.development.local -- next dev",
    "dev:next:react-scan": "dotenvx run -f .env.development.local -- next dev & npx react-scan@latest localhost:3000",
    "lint": "npm run lint:next && npm run lint:ts",
    "lint:next": "dotenvx run -f .env.development.local -- next lint",
    "lint:ts": "dotenvx run -f .env.development.local -- tsc --noEmit",
    "prod-simulate:next:react-scan": "rm -rf .next & dotenvx run -f .env.prod-simulate.local -- next build & next start & npx react-scan@latest localhost:3000",
    "prod-simulate:build": "dotenvx run -f .env.prod-simulate.local -- npm run registry:generate && next build",
    "prod-simulate:start": "dotenvx run -f .env.prod-simulate.local -- next start",
    "prod:build": "npm run registry:generate && next build",
    "prod:start": "next start",
    "registry:generate": "npm run registry:generate:static-data && npm run registry:generate:shadcn-public-files",
    "registry:generate:static-data": "vite-node registry/output-static-generator/scripts/generate-all-static-files.ts",
    "registry:generate:shadcn-public-files": "shadcn build registry/output-static-artifacts/shadcn-cli/registry.json",
    "registry:test": "vitest --watch --root registry/input/items"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.49.0",
    "@icons-pack/react-simple-icons": "^13.8.0",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@shikijs/rehype": "^3.8.1",
    "@tanstack/react-query": "^5.83.0",
    "@vercel/analytics": "^1.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "jotai": "^2.12.5",
    "lucide-react": "^0.525.0",
    "next": "15.4.2",
    "next-themes": "^0.4.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rehype-raw": "^7.0.0",
    "rehype-stringify": "^10.0.1",
    "remark": "^15.0.1",
    "remark-code-title": "^0.2.5",
    "remark-gfm": "^4.0.1",
    "remark-rehype": "^11.1.2",
    "remark-toc": "^9.0.0",
    "shadcn": "2.9.2",
    "shiki": "^3.8.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "usehooks-ts": "^3.1.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.2",
    "postcss": "^8",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5",
    "vite": "^7.0.5",
    "vite-node": "^3.2.4",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@sefinek/random-emoji": "^2.1.0",
    "@types/express": "^5.0.3",
    "date-fns": "^4.1.0",
    "express": "^5.1.0",
    "pretty-ms": "^9.2.0"
  },
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad"
}